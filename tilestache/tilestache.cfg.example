{

	"cache": {
		"path": "/var/tiles", 
		"name": "Test"
	},

	"layers": {

		"microsoft_aerial": {
			"provider": {
				"name": "proxy", 
				"provider": "MICROSOFT_AERIAL"
			}
		},

		"dithered": {
			"provider": {
				"class": "Atkinstache.dithering.Provider",
				"kwargs": {
					"source_layer": "microsoft_aerial"
				}
			}
		},

		"buildings": {
			"provider": {
				"class": "mapequalsyes.footprint.Provider",
				"kwargs": {
					"type": "way",
					"query": "[building=*]",
					"fill": "#ffffff"
				}
			}
		},

		"buildings-outline": {
			"provider": {
				"class": "mapequalsyes.footprint.Provider",
				"kwargs": {
					"type": "way",
					"query": "[building=*]",
					"fill": "#000000",
					"filltype": "line"
				}
			}
		},

		"buildingequalyes": {
			"provider": {
				"class": "TileStache.Goodies.Providers.Composite:Provider",
				"kwargs": {
					"stack": [
						{"src": "dithered"},
						{"src": "buildings"},
						{"src": "buildings-outline"}
					]
				}
				}
		}

	}
}
